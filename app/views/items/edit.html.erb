<%= simple_form_for(@item) do |f| %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :category %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :postal_code %>
  <%= f.input :quantity %>
  <%= f.input :min_days_rent %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>
